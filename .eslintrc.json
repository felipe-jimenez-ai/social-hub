{
  "extends": [
    "next",
    "next/core-web-vitals"
  ],
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "rules": {
    "react/no-unescaped-entities": 0,
    "@next/next/no-img-element": 0,
    "react-hooks/exhaustive-deps": "warn",
    "@typescript-eslint/no-unused-vars": "warn",
    "prefer-const": "warn"
  }
}